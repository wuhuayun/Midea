    <property name="createUser" column="createUser" length="20"/>
    <property name="createTime" column="createTime" length="20"/>
    <property name="modifyUser" column="modifyUser" length="20"/>
    <property name="modifyTime" column="modifyTime" length="20"/>
    <property name="vendorId">
      <column name="vendorId" sql-type="int default 0"/>
    </property>
    <property name="vendorSiteCode" column="vendorSiteCode" length="20"/>
    <property name="vendorSiteCodeAlt" column="vendorSiteCodeAlt" length="240"/>
  </class>
</hibernate-mapping> 
