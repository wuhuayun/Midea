<?xml version="1.0" encoding="utf-8"?>
<Root>
  <Name>美的中央空调供应链物流智能化管理系统</Name>
  <HasCMS>true</HasCMS>
  <Name2>思谷</Name2>
  <Url>
  </Url>
  <!--站点名称(后台可配置)-->
  <ICP>京ICP备*****号-1</ICP>
  <SiteID>
  </SiteID>
  <!--站点ICP号(后台可配置)-->
  <Keywords>诚威</Keywords>
  <!--站点关键字(后台可配置)-->
  <Description>YnCMS基于微软.NET4.0架构</Description>
  <!--站点简介(后台可配置)-->
  <AllowReg>1</AllowReg>
  <!--站点是否允许注册(后台可配置)-->
  <CheckReg>0</CheckReg>
  <!--注册是否需要审核(后台可配置)-->
  <IsHtml>1</IsHtml>
  <!--站点是否静态化(后台可配置)-->
  <StaticExt>.shtml</StaticExt>
  <!--静态文件后缀名-->
  <DefaultPoints>0</DefaultPoints>
  <!--注册会员赠送的博币-->
  <AdminGroupId>5</AdminGroupId>
  <!--管理员组的编号-->
  <TitleTail> - Powered by YnCMS</TitleTail>
  <CreatePages>15</CreatePages>
  <!--静态化的的栏目页/专题页数量，没有更多-->
  <UrlReWriter>1</UrlReWriter>
  <!--Url重写：0代表No;1代表Yes-->
  <Founders>..</Founders>
  <!--网站系统管理员，以.结尾，以.分割-->
  <ExecuteSql>1</ExecuteSql>
  <!--是否允许执行SQL，0表示只有在服务器上执行,1表示可以远程执行-->
  <SustainFLV>1</SustainFLV>
  <!--服务器支持FLV视频播放：0不支持,1表示支持-->
  <DebugKey>5E7D-8A8B-F75C-BFF3</DebugKey>
  <MailOnceCount>15</MailOnceCount>
  <!--每个邮箱单次发信的收件人数量-->
  <MailTimeCycle>300</MailTimeCycle>
  <MailPrivateKey>2zr7fgkb</MailPrivateKey>
  <!--单个邮箱发信的间隔周期, 单位为秒-->
  <CookieDomain>
  </CookieDomain>
  <!--留空表示只作用当前域名，作用多个域名用;隔开-->
  <CookiePath>/</CookiePath>
  <!--不要随便修改-->
  <CookiePrev>yncmsV100</CookiePrev>
  <!--如果一个站点有多套cms，则要修改此值-->
  <CookieKeyCode>
  </CookieKeyCode>
  <!--加密密钥，功能暂留-->
  <ModuleList>article,photo,soft,video,document,paper,product</ModuleList>
  <!--模块列表-->
  <AdminCheckUserState>0</AdminCheckUserState>
  <!--后台登陆时是否确认前台的登录状况(add:2011-03-07)-->
  <MainSite>1</MainSite>
  <!--是否为主站，如果不是，那么采集后的图片带前缀(add:2011-08-10)-->
  <WanSite>1</WanSite>
  <!--是否为外网网站(add:2011-08-10)-->
  <!--网站版本(add:2011-10-10)-->
  <ProductMaxBuyCount>10</ProductMaxBuyCount>
  <!--一次最多能买的商品数(add:2011-10-20)-->
  <ProductMaxCartCount>5</ProductMaxCartCount>
  <!--购物车最多能存放的数量(add:2011-10-20)-->
  <ProductMaxOrderCount>5</ProductMaxOrderCount>
  <!--最大未付款的订单，当订单超过这个数就不允许再进行购买(add:2011-10-20)-->
  <PassportTheme>default</PassportTheme>
  <SiteDataSize>10000</SiteDataSize>
  <!--网站数量级，请勿手工改(add:2012-11-15)-->
</Root>